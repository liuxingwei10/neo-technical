# mkinitcpio preset file for rtai-kernel

########################################
# DO NOT EDIT THIS LINE:
source $pkgdir/etc/mkinitcpio.d/rtai-kernel.kver
########################################
ALL_config="/etc/mkinitcpio.conf"

PRESETS=('default' 'fallback')

default_image="/boot/rtai-initrd.img"

fallback_image="/boot/rtai-fallback.img" 
fallback_options="-S autodetect"